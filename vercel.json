{
  "version": 2,
  "functions": {
    "api/**/*": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "buildCommand": null,
  "devCommand": null,
  "installCommand": null,
  "framework": null,
  "outputDirectory": "out",
  "rewrites": [
    { "source": "/guide", "destination": "/guide.html" },
    { "source": "/(.*)", "destination": "/index.html" }
  ],
  "crons": [
    {
      "path": "/api/cron/game-cycle",
      "schedule": "* * * * *"
    }
  ]
} 